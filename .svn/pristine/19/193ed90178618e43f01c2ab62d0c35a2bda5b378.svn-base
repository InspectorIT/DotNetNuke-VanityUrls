 .iit_Admin { width: 100%; }

.iit_feedList ul li
{
    list-style-type:none;
    margin:0px;
    padding:.5em;
    font-size:12px;
    margin-bottom: .5em;
    box-shadow: 0 1px 3px #ccc;
    background-color: #fff;
    cursor: pointer;
    
}

.iit_feedList ul li:hover {
    background-color: #EBF5FD;
}

.iit_colorblock {
    background-color: #06A9EF;
    width: 1em;
    display: inline-block;

}

.iit_colorblock a { white-space: nowrap;}

.iit_headingLabel {
    font-size: 1.6em;
    font-weight: bold;
    margin-bottom: .8em;

}

.iit_feedList ul li a
{
    margin-left: 1.4em;
    display: block;
    color: #222222 !important;
    
}

.iit_LeftColumn { width: 300px;}

.iit_feedList ul 
{
    margin:0 !important;
    padding:0 !important;
    text-indent: 0;
}


.iit_feedList
{
    width:300px; 
    height:300px;
    overflow-x: hidden;
    overflow-y: scroll;
    margin-top: 1em;
}

.iit_listWrench {
    float: left;
    position: relative;
    top: 1px;
    margin-right: 8px;
}

.iit_FeedSelected {
    background-color: #D6E8F5 !important;
}

.iit_feedDetails 
{
    margin-left: 10px;
    width: 100%;
}

.iit_largeTextbox { width:98%;border-radius: 4px;border:1px solid #dcdcdc; box-shadow: 0 0 4px #888;height: 2em;padding: 0 .2em;margin-top: .2em;  }
.iit_smallTextbox { width:54%;border-radius: 4px;border:1px solid #dcdcdc; box-shadow: 0 0 4px #888;height: 2em;padding: 0 .2em;margin-top: .2em;  }
.iit_textArea { height:4em; padding: .2em; }

.iit_SQLBox { height:300px;}

.iit_saveButtons 
{
    float:right; 
    margin-top:10px;
}

.iit_saveButtons input 
{
    height:35px; 
    width:100px; 
    font-weight:bold;
}

.iit_alerts 
{
    float:left;
    display:none;
    padding:5px;
    font-weight:bold;
    font-size:14px;
    width:330px; 
    border-style:dashed;
    border-color:#00CC32;
    min-height:25px;
    background-color:#9CE665;
    margin:10px 0px;
}

.iit_CampaignLeft { width: 48%;float: left;margin-right: 4%;}
.iit_CampaignRight { width: 48%;float: right;margin-right: 0;}

.iit_CampaignLeft fieldset, .iit_CampaignRight fieldset { border-radius: 4px;}

.ui-timepicker-div .ui-widget-header { margin-bottom: 8px; }
.ui-timepicker-div dl { text-align: left; }
.ui-timepicker-div dl dt { height: 25px; margin-bottom: -25px; }
.ui-timepicker-div dl dd { margin: 0 10px 10px 65px; }
.ui-timepicker-div td { font-size: 90%; }
.ui-tpicker-grid-label { background: none; border: none; margin: 0; padding: 0; }

.ui-timepicker-rtl{ direction: rtl; }
.ui-timepicker-rtl dl { text-align: right; }
.ui-timepicker-rtl dl dd { margin: 0 65px 10px 10px; }

.ui-combobox {
    position: relative;
    display: block;
    width: 100%;
    margin-bottom: 6px;
  }
  .ui-combobox-toggle {
    position: absolute;
    top: 9px;
    padding: 0;
      margin: 0;
    bottom: 0;
    margin-left: -1px;
    
      height: 1.8em;
    /* adjust styles for IE 6/7 */
      *height: 1.8em;
      *top: 0;
  }
  .ui-combobox-input {
    margin: 0;
    padding: 0.3em;
      width: 84%;
  }

  .ui-widget-content {
      border: 1px solid #AAA;
      background: white;
      color: #222;
  }

  .ui-menu-item { list-style-type: none !important;cursor: pointer;padding-bottom: 7px !important;}
  .ui-menu-item a { cursor: pointer;}

  .remove-autocomplete {
      background-color: red;
      font-weight: bold;
      color: transparent;
      padding: 2px;
      cursor: pointer;
      background: url('images/trash-can-delete.png') center no-repeat;
  }

  .validationFailure {background-color:#FFA9A9}

  .iit_footerLeft {
      float: left;
      width: 48%;
      margin-right: 8px;
  }

  .iit_footerRight {
      float: left;
      width: 48%;
  }